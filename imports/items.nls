extensions [csv]
; Shelf patches
; if id = -1, then it is not a shelf
patches-own [
  product_id      ;  (PK) Unique Product ID
  product_price   ; (int) Price of product
  product_quantity        ; (int) Quantity of product
]

to fill-shelf [id price quantity]
  set product_id id
  set product_price price
  set product_quantity quantity
end